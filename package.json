{
   "name": "vite-vue-scaffold",
   "version": "0.0.0",
   "private": true,
   "scripts": {
      "dev": "cross-env NODE_ENV=development vite",
      "build": "vite build",
      "start": "npm run dev",
      "lint:eslint": "eslint \"src/**/*.{ts,tsx,js,json,vue}\" --fix",
      "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,ts,css,scss,vue,html,md}\"",
      "lint:style": "stylelint \"**/*.{vue,css,scss,sass}\" \"!dist/**/*\"",
      "test": "jest"
   },
   "dependencies": {
      "core-js": "^3.6.5",
      "vue": "^3.0.5",
      "vue-class-component": "^8.0.0-0",
      "vue-router": "^4.0.2",
      "vuex": "^4.0.0-rc.2"
   },
   "devDependencies": {
      "@types/mockjs": "^1.0.3",
      "@typescript-eslint/eslint-plugin": "^4.11.1",
      "@typescript-eslint/parser": "^4.11.1",
      "@vitejs/plugin-vue": "^1.1.4",
      "@vue/compiler-sfc": "^3.0.4",
      "@vue/eslint-config-prettier": "^6.0.0",
      "@vue/eslint-config-typescript": "^5.0.2",
      "@vue/test-utils": "^2.0.0-beta.13",
      "@vuedx/typecheck": "^0.6.3",
      "@vuedx/typescript-plugin-vue": "^0.6.3",
      "cross-env": "^7.0.3",
      "dotenv": "^8.2.0",
      "eslint": "^7.16.0",
      "eslint-config-prettier": "^7.1.0",
      "eslint-plugin-prettier": "^3.3.0",
      "eslint-plugin-vue": "^7.4.0",
      "husky": "^5.0.9",
      "jest": "^26.6.3",
      "lint-staged": "^10.5.4",
      "mockjs": "^1.1.0",
      "prettier": "^2.2.1",
      "sass": "^1.32.7",
      "stylelint": "^13.8.0",
      "stylelint-config-prettier": "^8.0.2",
      "stylelint-config-standard": "^20.0.0",
      "stylelint-order": "^4.1.0",
      "ts-jest": "^26.4.4",
      "typescript": "^4.1.5",
      "vite": "^2.0.0-beta.65",
      "vue-eslint-parser": "^7.3.0",
      "vue-jest": "^5.0.0-0"
   },
   "lint-staged": {
      "*.{js,jsx,vue,ts,tsx}": "npm run lint:eslint",
      "*.{vue,css,scss,sass}": "npm run lint:style",
      "*.{js,json,tsx,ts,css,scss,vue,html,md}": "npm run lint:prettier"
   }
}
